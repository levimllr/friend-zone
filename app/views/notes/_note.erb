<li id="note-<%= note.id %>">
  <span class="timestamp">
    <%= note.created_at.strftime("%A, %B %e") %>
    (<%= time_ago_in_words(note.created_at) %> ago).
  </span>
  <span class="note_details">
    <%= note.stringify %>
  </span>
  <span class="title">
    <%= note.title %>
  </span>
  <span class="content">
    <%= note.content %>
  </span>
</li>